{
  "name": "vue-use-sync-url",
  "version": "1.0.1",
  "description": "sync url vue hook",
  "type": "module",
  "keywords": [
    "vue",
    "vue use",
    "sync to url",
    "vue use sync url"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && tsc -p tsconfig.build.json",
    "preview": "vite preview",
    "build-examples": "vite build --config gh-pages.config.js && cp dist-examples/index.html dist-examples/404.html",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --fix",
    "format": "prettier --write ./**/*.{vue,ts,tsx,js,jsx,css,less,scss,json,md}"
  },
  "devDependencies": {
    "@types/node": "^18.6.5",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "element-plus": "^2.2.12",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.6",
    "vue-tsc": "^0.38.4"
  },
  "peerDependencies": {
    "vue-router": ">=4.0.0"
  }
}
